type UserModel {
    email: ID!
    mobile: String
    nickname: String!
    status: UserStatus!
    token: String!
    addresses: [AddressModel!]
}

type AddressModel {
    postCode: String
    country: String!
    city: String!
    street: String!
    contact: String
    addressee: String!
}

enum UserStatus {
    ACTIVE
    INACTIVE
    TERMINATED
}