type UserDto {
    email: ID!
    mobile: String
    nickname: String!
    addresses: [AddressDto!]
    rating: Float
}

type AddressDto {
    postCode: String
    country: String!
    city: String!
    street: String!
    contact: String
    addressee: String!
}

type CategoryDto {
    id: String!
    description: String!
    ancestors: [CategoryDto!]
}

enum UserStatus {
    ACTIVE
    INACTIVE
    TERMINATED
}