input CreateUserModel {
    # A unique email
    email: ID!
    mobile: String
    # A unique nickname
    nickname: String!
    password: String!
    addresses: [CreateAddressModel!]
}

input CreateAddressModel {
    postCode: String
    country: String!
    city: String!
    street: String!
    contact: String
    addressee: String!
}

input CredentialsModel {
    email: ID!
    password: String!
}