input CreateUserDto {
    # A unique nickname
    nickname: String!
    # A unique email
    email: ID!
    password: String!
    role: Role!
    mobile: String
    addresses: [CreateAddressDto!]
}

input CreateAddressDto {
    postCode: String
    country: String!
    city: String!
    street: String!
    contact: String
    addressee: String!
}

input CredentialsDto {
    email: ID!
    password: String!
}

enum Role {
    ROLE_SELLER
    ROLE_BUYER
}